{
  "name": "NodeTerm",
  "version": "1.6.2",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "download-fonts": "node scripts/download-fonts.js",
    "build": "npm run download-fonts && webpack --mode production",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation concurrently \"webpack --watch --mode development\" \"wait-on dist/index.html -t 60000 && electron .\"",
    "pack": "npm run build && electron-builder --dir",
    "dist": "npm run build && npm prune --omit=optional --no-audit --no-fund && electron-builder",
    "dist:linux": "npm run build && electron-builder --linux",
    "scan:virustotal": "node scripts/virustotal-scan.js",
    "dist:scan": "npm run dist && npm run scan:virustotal",
    "version:patch": "npm version patch --no-git-tag-version",
    "version:minor": "npm version minor --no-git-tag-version",
    "version:major": "npm version major --no-git-tag-version",
    "version:build": "npm run build && npm run pack"
  },
  "build": {
    "appId": "com.electron.nodeterm",
    "productName": "NodeTerm",
    "copyright": "Copyright © 2024",
    "compression": "maximum",
    "files": [
      "dist/",
      "binaries/**",
      "main.js",
      "preload.js",
      "system-stats-worker.js",
      "package.json",
      "src/main/**",
      "src/services/**",
      "src/utils/**",
      "src/components/bastion-ssh.js",
      "node_modules/**/*",
      "!**/*.{md,markdown,ts,map,test,spec}",
      "!**/test/**",
      "!**/tests/**",
      "!**/__tests__/**",
      "!**/docs/**",
      "!**/doc/**",
      "!**/examples/**",
      "!**/example/**",
      "!**/.github/**",
      "!**/.git/**",
      "!**/LICENSE*",
      "!**/CHANGELOG*",
      "!**/README*",
      "!**/HISTORY*",
      "!**/CONTRIBUTING*",
      "!**/AUTHORS*",
      "!**/NOTICE*",
      "!**/*.d.ts",
      "!**/coverage/**",
      "!**/.nyc_output/**",
      "!**/.eslintrc*",
      "!**/.prettierrc*",
      "!**/tsconfig.json",
      "!**/jest.config.*",
      "!**/.travis.yml",
      "!**/.circleci/**",
      "!node_modules/electron/**",
      "!node_modules/electron-builder/**",
      "!node_modules/app-builder-bin/**",
      "!node_modules/@electron/**",
      "!node_modules/@types/**",
      "!node_modules/@babel/**",
      "!node_modules/webpack/**",
      "!node_modules/webpack-cli/**",
      "!node_modules/babel-loader/**",
      "!node_modules/copy-webpack-plugin/**",
      "!node_modules/html-webpack-plugin/**",
      "!node_modules/css-loader/**",
      "!node_modules/style-loader/**",
      "!node_modules/concurrently/**",
      "!node_modules/cross-env/**",
      "!node_modules/wait-on/**",
      "!node_modules/electron-reloader/**",
      "!node_modules/baseline-browser-mapping/**",
      "!node_modules/buffer/**",
      "!node_modules/os-browserify/**",
      "!node_modules/path-browserify/**",
      "!node_modules/process/**",
      "!node_modules/querystring-es3/**",
      "!node_modules/stream-browserify/**",
      "!node_modules/url/**",
      "!node_modules/util/**",
      "!node_modules/crypto-browserify/**",
      "!node_modules/pdf-parse/**"
    ],
    "asarUnpack": [
      "binaries/**",
      "node_modules/ssh2-promise/**",
      "node_modules/ssh2/**"
    ],
    "extraResources": [
      {
        "from": "resources/cygwin64",
        "to": "cygwin64",
        "filter": [
          "**/*"
        ]
      }
    ],
    "win": {
      "target": "nsis",
      "icon": "src/assets/app-icon.png",
      "artifactName": "${productName}-Setup-${version}.${ext}",
      "verifyUpdateCodeSignature": false,
      "requestedExecutionLevel": "asInvoker"
    },
    "electronLanguages": [
      "en",
      "es"
    ],
    "nsis": {
      "oneClick": true,
      "perMachine": false,
      "allowToChangeInstallationDirectory": false,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "NodeTerm",
      "deleteAppDataOnUninstall": false,
      "differentialPackage": false,
      "warningsAsErrors": false
    },
    "linux": {
      "target": "AppImage",
      "category": "Utility",
      "icon": "src/assets/app-icon.png"
    },
    "publish": {
      "provider": "github",
      "owner": "kalidus",
      "repo": "NodeTerm"
    },
    "mac": {
      "target": [
        {
          "target": "dmg",
          "arch": [
            "x64",
            "arm64"
          ]
        },
        {
          "target": "dir",
          "arch": [
            "x64",
            "arm64"
          ]
        }
      ],
      "icon": "src/assets/app-icon.png",
      "category": "public.app-category.utilities"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "Aplicación de escritorio multiplataforma con Electron y PrimeReact",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@mozilla/readability": "^0.6.0",
    "@napi-rs/canvas": "^0.1.82",
    "@types/react-resizable": "^3.0.8",
    "@xterm/addon-clipboard": "^0.1.0",
    "@xterm/addon-fit": "^0.10.0",
    "@xterm/addon-image": "^0.8.0",
    "@xterm/addon-unicode11": "^0.8.0",
    "@xterm/addon-web-links": "^0.11.0",
    "@xterm/addon-webgl": "^0.18.0",
    "@xterm/xterm": "^5.5.0",
    "assert": "^2.1.0",
    "basic-ftp": "^5.0.5",
    "browserify-zlib": "^0.2.0",
    "dommatrix": "^0.1.1",
    "dompurify": "^3.3.0",
    "electron-log": "^5.4.3",
    "electron-updater": "^6.6.2",
    "guacamole-common-js": "^1.5.0",
    "guacamole-lite": "^1.0.2",
    "highlight.js": "^11.11.1",
    "https-browserify": "^1.0.0",
    "jsdom": "^27.1.0",
    "kdbxweb": "^2.1.1",
    "marked": "^16.4.1",
    "node-pty": "^1.1.0-beta34",
    "node-ssh": "^13.2.1",
    "pako": "^2.1.0",
    "primeflex": "^3.3.1",
    "primeicons": "^6.0.1",
    "primereact": "^10.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.5.0",
    "react-resizable": "^3.0.5",
    "react-split": "^2.0.14",
    "ssh2-promise": "^1.0.3",
    "ssh2-sftp-client": "^12.0.1",
    "stream-http": "^3.2.0",
    "systeminformation": "^5.27.7",
    "turndown": "^7.2.2",
    "unzipper": "^0.12.3",
    "ws": "^8.18.0",
    "xterm": "^5.3.0",
    "xterm-addon-fit": "^0.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.6",
    "@babel/preset-env": "^7.23.6",
    "@babel/preset-react": "^7.23.3",
    "babel-loader": "^9.1.3",
    "baseline-browser-mapping": "^2.9.11",
    "buffer": "^6.0.3",
    "concurrently": "^8.2.2",
    "copy-webpack-plugin": "^13.0.0",
    "wait-on": "^8.0.1",
    "cross-env": "^10.0.0",
    "crypto-browserify": "^3.5.1",
    "css-loader": "^6.8.1",
    "electron": "^38.1.2",
    "electron-builder": "^26.0.12",
    "electron-reloader": "^1.2.3",
    "html-webpack-plugin": "^5.6.3",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "process": "^0.11.10",
    "querystring-es3": "^0.2.1",
    "stream-browserify": "^3.0.0",
    "style-loader": "^3.3.3",
    "url": "^0.11.4",
    "util": "^0.12.5",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "optionalDependencies": {
    "dmg-license": "^1.0.11"
  },
  "overrides": {
    "ssh2": "1.17.0"
  }
}
