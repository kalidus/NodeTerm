{
  "name": "NodeTerm",
  "version": "1.5.3",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "webpack --mode production",
    "dev": "concurrently \"webpack --watch --mode development\" \"electron .\"",
    "pack": "npm run build && electron-builder --dir",
    "dist": "npm run build && npm prune --omit=optional --no-audit --no-fund && electron-builder",
    "dist:linux": "npm run build && electron-builder --linux",
    "version:patch": "npm version patch --no-git-tag-version",
    "version:minor": "npm version minor --no-git-tag-version",
    "version:major": "npm version major --no-git-tag-version",
    "version:build": "npm run build && npm run pack"
  },
  "build": {
    "appId": "com.electron.nodeterm",
    "productName": "NodeTerm",
    "files": [
      "dist/",
      "binaries/**",
      "src/components/",
      "src/utils/",
      "src/services/",
      "node_modules/",
      "main.js",
      "preload.js",
      "system-stats-worker.js",
      "package.json"
    ],
    "asarUnpack": [
      "binaries/**",
      "node_modules/ssh2-promise/**",
      "node_modules/ssh2/**"
    ],
    "win": {
      "target": "nsis",
      "icon": "src/assets/app-icon.png"
    },
    "linux": {
      "target": "AppImage",
      "category": "Utility",
      "icon": "src/assets/app-icon.png"
    },
    "nsis": {
      "oneClick": true,
      "perMachine": false
    },
    "publish": {
      "provider": "github",
      "owner": "kalidus",
      "repo": "NodeTerm"
    },
    "mac": {
      "target": [
        {
          "target": "dmg",
          "arch": ["x64", "arm64"]
        },
        {
          "target": "dir",
          "arch": ["x64", "arm64"]
        }
      ],
      "icon": "src/assets/app-icon.png",
      "category": "public.app-category.utilities"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "Aplicaci√≥n de escritorio multiplataforma con Electron y PrimeReact",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@types/react-resizable": "^3.0.8",
    "@xterm/addon-clipboard": "^0.1.0",
    "@xterm/addon-fit": "^0.10.0",
    "@xterm/addon-image": "^0.8.0",
    "@xterm/addon-unicode11": "^0.8.0",
    "@xterm/addon-web-links": "^0.11.0",
    "@xterm/addon-webgl": "^0.18.0",
    "@xterm/xterm": "^5.5.0",
    "assert": "^2.1.0",
    "browserify-zlib": "^0.2.0",
    "guacamole-common-js": "^1.5.0",
    "guacamole-lite": "^1.0.2",
    "https-browserify": "^1.0.0",
    "node-pty": "^1.1.0-beta34",
    "node-ssh": "^13.2.1",
    "primeflex": "^3.3.1",
    "primeicons": "^6.0.1",
    "primereact": "^10.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.5.0",
    "react-resizable": "^3.0.5",
    "react-split": "^2.0.14",
    "ssh2-promise": "^1.0.3",
    "ssh2-sftp-client": "^12.0.1",
    "stream-http": "^3.2.0",
    "systeminformation": "^5.27.7",
    "unzipper": "^0.12.3",
    "ws": "^8.18.0",
    "xterm": "^5.3.0",
    "xterm-addon-fit": "^0.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.6",
    "@babel/preset-env": "^7.23.6",
    "@babel/preset-react": "^7.23.3",
    "babel-loader": "^9.1.3",
    "buffer": "^6.0.3",
    "concurrently": "^8.2.2",
    "copy-webpack-plugin": "^13.0.0",
    "crypto-browserify": "^3.12.1",
    "css-loader": "^6.8.1",
    "electron": "^28.2.0",
    "electron-builder": "^24.9.1",
    "electron-reloader": "^1.2.3",
    "html-webpack-plugin": "^5.6.3",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "process": "^0.11.10",
    "querystring-es3": "^0.2.1",
    "stream-browserify": "^3.0.0",
    "style-loader": "^3.3.3",
    "url": "^0.11.4",
    "util": "^0.12.5",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "optionalDependencies": {
    "dmg-license": "^1.0.11"
  },
  "overrides": {
    "ssh2": "1.16.0"
  }
}
